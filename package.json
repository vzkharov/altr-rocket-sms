{
	"name": "@alterdepression/rocketsms",
	"license": "MIT",
	"version": "1.0.0",
	"author": "Vadzim Zakharov <zakharovvadzim@yandex.by>",
	"description": "A client to rocket-sms API...",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"module": "./lib/index.mjs",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vzkharov/altr-rocket-sms.git"
	},
	"scripts": {
		"build": "tsup",
		"test": "bun test src --coverage --timeout 5000",
		"clean": "rm -rf .turbo node_modules lib",
		"check": "bunx @biomejs/biome check --apply ./src",
		"format": "bunx @biomejs/biome check --apply ./src",
		"prepublishOnly": "bun run build"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.2",
		"@morlay/bunpublish": "^0.3.0",
		"@types/bun": "^1.1.1",
		"@types/node": "^20.12.7",
		"bun-plugin-dts": "^0.2.3",
		"bun-types": "^1.1.7",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"lib/**"
	],
	"exports": {
		".": {
			"import": "./lib/index.mjs",
			"require": "./lib/index.js"
		}
	},
	"typesVersions": {
		"*": {
			".": [
				"./lib/index.d.ts"
			]
		}
	},
	"keywords": [
		"redis",
		"totp",
		"auth",
		"edge",
		"upstash"
	],
	"bugs": {
		"url": "https://github.com/vzkharov/altr-auth/issues"
	},
	"homepage": "https://github.com/vzkharov/altr-auth#readme",
	"trustedDependencies": [
		"@biomejs/biome"
	],
	"directories": {
		"lib": "lib"
	}
}
